# - Config file for the LillyDAP package
# It defines the following variables
#  LillyDAP_INCLUDE_DIRS    - include directories for LillyDAP
#  LillyDAP_LIBRARIES       - libraries to link against (e.g. -lquickder)
 
# Compute paths
get_filename_component (LillyDAP_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set (LillyDAP_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")
 
set (LillyDAP_LIBRARIES lillydap)
